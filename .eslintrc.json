{
    "env": {
        "browser": true,
        "es2022": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        }
    },
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-undef" : [
            "warn"
        ],
        "no-unused-vars" : [
            "warn"
        ],
        "no-var" : [
            "warn"
        ]
    }
}
